<% if @qcareer_configurations.any? %>
	// Fill the right box with first qcareer_configuration details in thqcareer_configuratione list
	$('#div_qcareer_configuration_details').html("<%= escape_javascript(render(:partial=>'show')) %>");

	// Rendering the item list
	$('#div_qcareer_configuration_index').html("<%= escape_javascript(render(:partial=>"index")) %>");
	$('#div_qcareer_configuration_drop_down').html("<%= escape_javascript(theme_drop_down(@qcareer_configurations, :name)) %>");
<% else %>
	var noResultsText = "<%= escape_javascript(theme_panel_message(I18n.translate('forms.no_results')))%>";
	$('#div_qcareer_configuration_index').html(noResultsText);
	$('#div_qcareer_configuration_details').html(noResultsText);
<% end %>