<% if @events.any? %>
	// Rendering the item list
	$('#div_event_index').html("<%= escape_javascript(render(:partial=>'index')) %>");
<% else %>
	var noResultsText = "<%= escape_javascript(theme_panel_message(I18n.translate('forms.no_results')))%>";
	$('#div_event_index').html(noResultsText);
<% end %>